<div class="container" style="margin:0 auto;">
    <h1>Articulos</h1>
        <div class="row">
            <%= render @articles %>
        </div>
        <div class="col-md field top-space">
            <% if user_signed_in?%>
                <%= link_to "Nuevo Articulo", new_article_path,class:" button "%>
            <%end%>
            <div class="top-space text-center">
                <%= will_paginate @articles %>
            </div>
        </div>
</div>
